version_pattern='sed -rn '\''/\/archive\/.*\.tar\.gz.*"nofollow"/{s,.*/archive/(.*)\.tar.*,\\1,;s/^v//;p}'\''|sort -rV'
help_text="To be used with https://github.com/USER/PROJECT/releases url."
download_pattern=$(echo "$dl_url"|sed -r 's,/[^/]+/releases/?,,')'/${pkgname}/archive/${pkgver}.tar.gz'

