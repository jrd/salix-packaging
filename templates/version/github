version_pattern = r"""sed -rn '/\/archive\/.*\.tar\.gz.*"nofollow"/{s,.*/archive/(.*)\.tar.*,\1,;s/^v//;p}'"""
help_text = 'To be used with https://github.com/USER/PROJECT/releases url.'
download_pattern = 'https://github.com/{pkgname}/{pkgname}/archive/{pkgver}.tar.gz'

